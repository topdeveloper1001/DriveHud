            Width="50"
            Grid.Row="2"
            Maximum="100"
            TabIndex="5"
            Minimum="0" />
        <xctk:DoubleUpDown
            FormatString="F1"
            x:Name="txtWwsfMin"
            Grid.Column="2"
            HorizontalAlignment="Left"
            Margin="8,0,0,0"
            VerticalAlignment="Center"
            Width="50"
            Grid.Row="3"
            Maximum="100"
            TabIndex="6"
            Minimum="0" />
        <TextBlock
            Grid.Column="3"
            Margin="5,0,0,0"
            TextWrapping="Wrap"
            Text="to"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            Foreground="White"
            Grid.Row="3" />
        <xctk:DoubleUpDown
            FormatString="F1"
            x:Name="txtWwsfMax"
            Grid.Column="4"
            HorizontalAlignment="Left"
            Margin="8,0,0,0"
            VerticalAlignment="Center"
            Width="50"
            Grid.Row="3"
            Maximum="100"
            TabIndex="7"
            Minimum="0" />
        <xctk:DoubleUpDown
            FormatString="F1"
            x:Name="txtPfrMin"
            Grid.Column="6"
            HorizontalAlignment="Left"
            Margin="8,0,0,0"
            VerticalAlignment="Center"
            Width="50"
            Maximum="100"
            TabIndex="8"
