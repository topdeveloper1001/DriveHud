<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:AcePokerSolutions_PlayerXRay_CustomControls="clr-namespace:DriveHUD.PlayerXRay.CustomControls"    
    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
    xmlns:System="clr-namespace:System;assembly=mscorlib"
    x:Class="DriveHUD.PlayerXRay.UserControls.Notes.PlayerSettingUC"
    mc:Ignorable="d"
    d:DesignHeight="160"
    d:DesignWidth="590"
    VerticalAlignment="Top"
    Margin="0,5"
    Background="#35FFFFFF"
    Height="160">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition
                Width="Auto" />
            <ColumnDefinition
                Width="Auto" />
            <ColumnDefinition
                Width="Auto" />
            <ColumnDefinition
                Width="Auto" />
            <ColumnDefinition
                Width="Auto" />
            <ColumnDefinition
                Width="Auto" />
            <ColumnDefinition
                Width="Auto" />
            <ColumnDefinition
                Width="Auto" />
            <ColumnDefinition
                Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Image
            x:Name="imgPlayer"
            Margin="10,5,10,0"
            Width="128"
            Height="128"
            Source="/Player X-Ray;component/Resources/PLR_LAG1.jpg"
            Grid.RowSpan="4"
            VerticalAlignment="Top" />
        <TextBlock
            Grid.Column="1"
            Margin="5,0,0,0"
            TextWrapping="Wrap"
            Text="VPIP%:"
            d:LayoutOverrides="Width, Height"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            Foreground="White"
            FontWeight="Bold" />
        <TextBlock
            Grid.Column="1"
            Margin="5,0,0,0"
            TextWrapping="Wrap"
            Text="AGG:"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            Foreground="White"
            FontWeight="Bold"
            Grid.Row="1" />
        <TextBlock
            Grid.Column="1"
            Margin="5,0,0,0"
            TextWrapping="Wrap"
            Text="WTSD"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            Foreground="White"
            FontWeight="Bold"
            Grid.Row="2" />
        <TextBlock
            Grid.Column="1"
            Margin="5,0,0,0"
            TextWrapping="Wrap"
            Text="W$WSF:"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            Foreground="White"
            FontWeight="Bold"
            Grid.Row="3" />
        <TextBlock
            Grid.Column="5"
            Margin="25,0,0,0"
            TextWrapping="Wrap"
            Text="PFR:"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            Foreground="White"
            FontWeight="Bold" />
        <TextBlock
            Grid.Column="5"
            Margin="25,0,0,0"
            TextWrapping="Wrap"
            Text="3-bet:"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            Foreground="White"
            FontWeight="Bold"
            Grid.Row="1" />
        <TextBlock
            Grid.Column="5"
            Margin="25,0,0,0"
            TextWrapping="Wrap"
            Text="W$SD:"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            Foreground="White"
            FontWeight="Bold"
            Grid.Row="2" />
        <xctk:DoubleUpDown            
            FormatString="F1"
            x:Name="txtVpIpMin"
            Grid.Column="2"
            HorizontalAlignment="Right"
            VerticalAlignment="Center"
            Width="50"
            Maximum="100"
            TabIndex="0"
            Minimum="0" />
        <TextBlock
            Grid.Column="3"
            Margin="5,0,0,0"
            TextWrapping="Wrap"
            Text="to"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            Foreground="White" />
        <xctk:DoubleUpDown            
            FormatString="F1"
            x:Name="txtVpIpMax"
            Grid.Column="4"
            HorizontalAlignment="Left"
            Margin="8,0,0,0"
            VerticalAlignment="Center"
            Width="50"
            Maximum="100"
            TabIndex="1"
            Minimum="0" />
        <xctk:DoubleUpDown            
            FormatString="F1"
