            HorizontalAlignment="Left"
            Margin="8,0,0,0"
            VerticalAlignment="Center"
            Width="50"
            Grid.Row="2"
            Maximum="100"
            TabIndex="4"
            Minimum="0" />
        <TextBlock
            Grid.Column="3"
            Margin="5,0,0,0"
            TextWrapping="Wrap"
