            Foreground="White" />
        <xctk:DoubleUpDown
            FormatString="F1"
            x:Name="txtPfrMax"
            Grid.Column="8"
            HorizontalAlignment="Left"
            Margin="8,0,0,0"
            VerticalAlignment="Center"
            Width="50"
            Maximum="100"
            TabIndex="9"
            Minimum="0" />
