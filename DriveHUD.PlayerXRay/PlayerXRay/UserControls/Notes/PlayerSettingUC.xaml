            Minimum="0" />
        <TextBlock
            x:Name="lblDescription"
            Margin="5,5,0,0"
            TextWrapping="Wrap"
            Text="W$WSF:"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            Foreground="White"
            FontWeight="Bold"
            Grid.Row="3" />
        <CheckBox
            x:Name="chkInclude"
            Content=""
            Grid.Column="8"
            HorizontalAlignment="Right"
            Margin="0,5,5,0"
            VerticalAlignment="Top"
            d:LayoutOverrides="Height"
            TabIndex="14" />
    </Grid>
</UserControl>
