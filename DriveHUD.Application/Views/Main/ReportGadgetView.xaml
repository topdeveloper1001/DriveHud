                Grid.ColumnSpan="3"
                Stretch="Fill"
                Grid.Row="1"
                VerticalAlignment="Top"
                Height="1"
                Fill="#4971c4" />
            <Rectangle
                Grid.ColumnSpan="3"
                Grid.Row="3"
                Stretch="Fill"
                Fill="#232528" />

            <ListBox
                x:Name="ReportRadioButtonPanel"
                Grid.Row="1"
                Grid.Column="1"
                Height="51"
                Style="{StaticResource ListBoxWithAutoScrollHorizontal}">
                <ListBox.Items>
                    <!-- Cash Columns -->
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="OverAll"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.OverAll}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowCashData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="Position"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.Position}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowCashData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="Sessions"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.Session}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowCashData, Converter={dh:BoolToVisibilityConverter}}" />

                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="Stakes"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.Stake}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowCashData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="Holecards"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.HoleCards}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowCashData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="Time"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.Time}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowCashData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="{ResX Key=Common_ShowdownHands}"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.ShowdownHands}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowCashData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="Poker Site"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.PokerSite}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowCashData, Converter={dh:BoolToVisibilityConverter}}" />
                    <!--<RadioButton Style="{StaticResource ReportRadioButton}" Content="Opponent Analysis" Width="150" Height="51"
                             IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.OpponentAnalysis}}"
                             Command="{Binding ReportRadioButtonClickCommand}"
                             Visibility="{Binding IsShowCashData, Converter={dh:BoolToVisibilityConverter}}" />-->

                    <!-- Tournament Columns -->
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="{ResX Key=Common_Results}"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.TournamentResults}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowTournamentData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="{ResX Key=Common_Tournaments}"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.Tournaments}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowTournamentData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="{ResX Key=Common_Stats}"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.TournamentStats}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowTournamentData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="{ResX Key=Common_StackSize}"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.TournamentStackSizes}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowTournamentData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="{ResX Key=Common_Position}"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.TournamentPosition}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowTournamentData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="{ResX Key=Common_HoleCards}"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.TournamentHoleCards}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowTournamentData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="{ResX Key=Common_ShowdownHands}"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.TournamentShowdownHands}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowTournamentData, Converter={dh:BoolToVisibilityConverter}}" />
                    <RadioButton
                        Style="{StaticResource ReportRadioButton}"
                        Content="{ResX Key=Common_PokerSite}"
                        Width="150"
                        Height="51"
                        IsChecked="{Binding ReportSelectedItemStat, Converter={StaticResource ParameterToBoolConverter}, ConverterParameter={x:Static enums:EnumReports.TournamentPokerSite}}"
                        Command="{Binding ReportRadioButtonClickCommand}"
                        Visibility="{Binding IsShowTournamentData, Converter={dh:BoolToVisibilityConverter}}" />
                </ListBox.Items>
            </ListBox>

            <Grid
                Grid.Column="1"
                Grid.Row="2">
                <Grid.RowDefinitions>
                    <RowDefinition
                        Height="*" />
                    <RowDefinition
                        MinHeight="30"
                        Height="Auto" />
                </Grid.RowDefinitions>
                <telerik:RadGridView
                    x:Name="GridViewReport"
                    IsReadOnly="True"
                    Grid.Row="0"
                    MaxHeight="260"
                    VerticalAlignment="Stretch"
                    RowHeight="40"
                    SelectionMode="Single"
                    CanUserFreezeColumns="True"
                    CanUserResizeColumns="True"
                    CanUserReorderColumns="True"
                    RowLoaded="GridViewReport_RowLoaded"
                    ItemsSource="{Binding Path=ReportCollection, Mode=TwoWay}"
                    SelectedItem="{Binding Path=ReportSelectedItem, Mode=TwoWay}"
                    RowStyle="{StaticResource CustomSelectionIfSingleGridViewRowStyle}"
                    Sorting="GridViewReport_Sorting">
                    <telerik:RadContextMenu.ContextMenu>
                        <telerik:RadContextMenu
                            x:Name="GridViewReportMenu"
                            ItemContainerStyle="{StaticResource GridMenuItemStyle}">
                            <telerik:RadContextMenu.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <WrapPanel
                                        Orientation="Vertical"
                                        MaxHeight="470"
                                        Background="#181A1D" />
                                </ItemsPanelTemplate>
                            </telerik:RadContextMenu.ItemsPanel>
                        </telerik:RadContextMenu>
                    </telerik:RadContextMenu.ContextMenu>
                </telerik:RadGridView>

                <telerik:RadBusyIndicator
                    Grid.Row="0"
                    IsBusy="{Binding Path=IsBusy}"
                    Background="{StaticResource BackgroundBrush}"
                    Visibility="{Binding Path=IsBusy, Converter={dh:BoolToVisibilityConverter}}">
                    <telerik:RadBusyIndicator.OverlayStyle>
                        <Style
                            TargetType="Rectangle">
                            <Setter
                                Property="Fill"
                                Value="Transparent" />
                            <Setter
                                Property="Stroke"
                                Value="Transparent" />
                        </Style>
                    </telerik:RadBusyIndicator.OverlayStyle>
                    <telerik:RadBusyIndicator.ProgressBarStyle>
                        <Style
                            TargetType="telerik:RadProgressBar">
                            <Setter
                                Property="Foreground"
                                Value="{StaticResource BackgroundBlueBrush}" />
                            <Setter
                                Property="Background"
                                Value="{StaticResource BackgroundBrush}" />
                            <Setter
                                Property="BorderThickness"
                                Value="0" />
                            <Setter
                                Property="Maximum"
                                Value="100" />
                            <Setter
                                Property="IsTabStop"
                                Value="False" />
                            <Setter
                                Property="Margin"
                                Value="0,0,0,-20" />
                        </Style>
                    </telerik:RadBusyIndicator.ProgressBarStyle>
                    <telerik:RadBusyIndicator.BusyContentTemplate>
                        <DataTemplate />
                    </telerik:RadBusyIndicator.BusyContentTemplate>
                </telerik:RadBusyIndicator>

                <ItemsControl
                    Grid.Row="1"
                    Margin="5"
                    Background="{StaticResource BackgroundBrush1}"
                    ItemsSource="{Binding Path=CurrentlyBuiltFilter.FilterSectionCollection}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <Border
                                ToolTip="{Binding Path=Name}"
                                Height="30"
                                Width="100"
                                Background="{StaticResource BackgroundBrush3}"
                                Margin="3"
                                Visibility="{Binding Path=IsActive, Mode=TwoWay, Converter={dh:BoolToVisibilityConverter}}">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition
                                            Width="*" />
                                        <ColumnDefinition
                                            Width="24" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock
                                        Grid.Column="0"
                                        Margin="5"
                                        VerticalAlignment="Center"
                                        TextTrimming="CharacterEllipsis"
                                        TextWrapping="NoWrap">
                                        <Run
                                            Text="{Binding Path=Name}" />
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Column="1"
                                        Text="&#215;"
                                        Cursor="Hand"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Margin="5 0 5 5"
                                        FontSize="20">
                                        <TextBlock.Foreground>
                                            <LinearGradientBrush
                                                StartPoint="0,0"
                                                EndPoint="0,1">
                                                <LinearGradientBrush.GradientStops>
                                                    <GradientStopCollection>
                                                        <GradientStop
                                                            Color="#4568b4" />
                                                        <GradientStop
                                                            Color="#3f60b6"
                                                            Offset="1.0" />
                                                    </GradientStopCollection>
                                                </LinearGradientBrush.GradientStops>
                                            </LinearGradientBrush>
                                        </TextBlock.Foreground>
                                        <TextBlock.InputBindings>
                                            <MouseBinding
                                                MouseAction="LeftClick"
                                                Command="{Binding Path=DataContext.ButtonFilterModelSectionRemove_CommandClick, RelativeSource={RelativeSource AncestorType=ItemsControl}}"
                                                CommandParameter="{Binding}" />
                                        </TextBlock.InputBindings>
                                    </TextBlock>
                                </Grid>
                            </Border>

                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <WrapPanel
                                IsItemsHost="True" />
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>

            </Grid>

            <GridSplitter
                Grid.Row="3"
                HorizontalAlignment="Stretch"
                Height="5"
                Grid.ColumnSpan="3"
                Background="Transparent"
                DragDelta="GridSplitter_DragDelta" />

            <Grid
                x:Name="HandHistoryArea"
                Grid.Row="4"
                Background="#181A1D"
                Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition
                        Height="58" />
                    <RowDefinition
                        Height="9" />
                    <RowDefinition
                        Height="*" />
                </Grid.RowDefinitions>

                <StackPanel
                    x:Name="StackPanelKnownHandsFilters"
                    Orientation="Horizontal"
                    Height="30">
                    <controls:ComboBoxEx
                        Width="180"
                        VerticalContentAlignment="Center"
                        HorizontalContentAlignment="Center"
                        ToggleButtonTemplate="{StaticResource ComboBoxToggleButton}"
                        ItemContainerStyle="{StaticResource ComboboxItemContainerBlueStyle}"
                        ItemsBackgroundColor="{StaticResource ComboboxBackGroundBrush}"
                        ItemsSource="{Binding FilterAmountDictionary}"
                        SelectedValue="{Binding FilterAmountDictionarySelectedItem}"
                        SelectedValuePath="Key"
                        DisplayMemberPath="Value" />

                    <CheckBox
                        Content="{ResX Key=Common_ReportView_ShowHoleCards}"
                        Margin="40,3,0,3"
                        IsChecked="{Binding ReplayerShowHolecards_IsChecked, Mode=TwoWay}" />
                    <CheckBox
                        Content="{ResX Key=Common_ReportView_TaggedHands}"
                        Margin="40,3,0,3"
                        IsChecked="{Binding FilterTaggedHands_IsChecked, Mode=TwoWay}" />

                    <controls:ComboBoxEx
                        Margin="10,0,0,0"
                        MinWidth="110"
                        Width="110"
                        ToggleButtonTemplate="{StaticResource ComboBoxToggleButton}"
                        ItemContainerStyle="{StaticResource ComboboxItemContainerBlueStyle}"
                        ItemsBackgroundColor="{StaticResource ComboboxBackGroundBrush}"
                        ItemsSource="{Binding FilterHandTagDictionary}"
                        SelectedValue="{Binding FilterHandTagSelectedItem}"
                        SelectedValuePath="Key"
                        DisplayMemberPath="Value" />

