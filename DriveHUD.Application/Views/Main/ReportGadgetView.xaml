                                            Property="Foreground"
                                            Value="White" />
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Style>
                    </Button>
                </StackPanel>

                <telerik:RadGridView
                    x:Name="GridViewKnownHands"
                    Grid.Row="2"
                    IsReadOnly="True"
                    CanUserResizeColumns="True"
                    CanUserReorderColumns="True"
                    RowHeight="40"
                    Foreground="Red"
                    SelectionMode="Single"
                    MouseDoubleClick="GridViewKnownHands_MouseDoubleClick"
                    ItemsSource="{Binding Path=ReportSelectedItemStatisticsCollection_Filtered}"
                    RowLoaded="GridViewKnownHands_RowLoaded"
                    RowStyle="{StaticResource GridViewKnownHandsRowStyle}"
                    AlternateRowStyle="{StaticResource GridViewKnownHandsAlternateRowStyle}"
                    AlternateRowBackground="{StaticResource GridView_AltRowBackground}"
                    EnableColumnVirtualization="True"
                    EnableRowVirtualization="True"
                    Sorting="GridViewKnownHands_Sorting">

                    <telerik:RadContextMenu.ContextMenu>
                        <telerik:RadContextMenu
                            x:Name="GridViewKnownHandsMenu"
                            ItemContainerStyle="{StaticResource GridMenuItemStyle}">

                        </telerik:RadContextMenu>
                    </telerik:RadContextMenu.ContextMenu>

                    <telerik:RadGridView.Columns>

                        <telerik:GridViewDataColumn
                            Header="{ResX Key=Common_ReportView_Hands_Date}"
                            Width="*"
                            MinWidth="{Binding Path=Header, RelativeSource={RelativeSource Self}, Converter={StaticResource TextToWidthConverter}}"
                            DataMemberBinding="{Binding Path=Statistic.Time, Converter={dh:DateTimeToLocalizedDateTimeConverter}}">
                        </telerik:GridViewDataColumn>

                        <telerik:GridViewDataColumn
                            Header="{ResX Key=Common_ReportView_Hands_Cards}"
                            UniqueName="Cards"
                            MinWidth="80"
                            Width="*"
                            DataMemberBinding="{Binding Path=PlayerCards}">
                            <telerik:GridViewDataColumn.CellTemplate>
                                <DataTemplate>
                                    <application:CardUC
                                        Cards="{Binding Path=PlayerCards.Cards}"
                                        HorizontalAlignment="Left" />
                                </DataTemplate>
                            </telerik:GridViewDataColumn.CellTemplate>
                        </telerik:GridViewDataColumn>

                        <telerik:GridViewDataColumn
                            Header="{ResX Key=Common_ReportView_Hands_Preflop}"
                            Width="*"
                            MinWidth="{Binding Path=Header, RelativeSource={RelativeSource Self}, Converter={StaticResource TextToWidthConverter}}"
                            DataMemberBinding="{Binding Path=Statistic.FacingPreflop}"
                            CellTemplateSelector="{StaticResource PreflopTemplateSelector}">

                        </telerik:GridViewDataColumn>

                        <telerik:GridViewDataColumn
                            Header="{ResX Key=Common_ReportView_Hands_Board}"
                            UniqueName="Board"
                            MinWidth="130"
                            Width="*"
                            DataMemberBinding="{Binding Path=BoardCards}">
                            <telerik:GridViewDataColumn.CellTemplate>
                                <DataTemplate>
                                    <Grid
                                        HorizontalAlignment="Left"
                                        Width="130">
                                        <application:CardUC
                                            Cards="{Binding Path=Statistic.Board}"
                                            HorizontalAlignment="Left" />
                                    </Grid>
                                </DataTemplate>
                            </telerik:GridViewDataColumn.CellTemplate>
                        </telerik:GridViewDataColumn>

                        <telerik:GridViewDataColumn
                            Header="{ResX Key=Common_ReportView_Hands_Line}"
                            Width="*"
                            MinWidth="{Binding Path=Header, RelativeSource={RelativeSource Self}, Converter={StaticResource TextToWidthConverter}}"
                            DataMemberBinding="{Binding Path=Statistic.Line}">
                        </telerik:GridViewDataColumn>

                        <telerik:GridViewDataColumn
                            Header="{ResX Key=Common_ReportView_Hands_NetWon}"
                            UniqueName="NetWon"
                            Width="*"
                            IsResizable="True"
                            MinWidth="{Binding Path=Header, RelativeSource={RelativeSource Self}, Converter={StaticResource TextToWidthConverter}}"
                            DataMemberBinding="{Binding Path=Statistic.NetWon}">
                            <telerik:GridViewDataColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock
                                        Text="{Binding Path=Statistic.NetWon, StringFormat=\{0:c2\}}"
                                        Foreground="{Binding Path=Statistic.NetWon, Converter={StaticResource ValueToColorConverter}}" />
                                </DataTemplate>
                            </telerik:GridViewDataColumn.CellTemplate>
                        </telerik:GridViewDataColumn>

                        <telerik:GridViewDataColumn
                            Header="{ResX Key=Common_ReportView_Hands_Position}"
                            Width="*"
                            MinWidth="{Binding Path=Header, RelativeSource={RelativeSource Self}, Converter={StaticResource TextToWidthConverter}}"
                            DataMemberBinding="{Binding Path=Statistic.Position}">
                            <telerik:GridViewDataColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock
                                        Text="{Binding Path=Statistic.PositionString}" />
                                </DataTemplate>
                            </telerik:GridViewDataColumn.CellTemplate>
                        </telerik:GridViewDataColumn>

                        <telerik:GridViewDataColumn
                            Header="{ResX Key=Common_ReportView_Hands_AllInEquity}"
                            UniqueName="Equity"
                            Width="*"
                            IsResizable="True"
                            MinWidth="{Binding Path=Header, RelativeSource={RelativeSource Self}, Converter={StaticResource TextToWidthConverter}}"
                            DataMemberBinding="{Binding Path=Statistic.Equity}">
                            <telerik:GridViewDataColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock
                                        Text="{Binding Path=Statistic.Equity, StringFormat=\{0:p2\}, Converter={StaticResource ValueToEquityConverter}}" />
                                </DataTemplate>
                            </telerik:GridViewDataColumn.CellTemplate>
                        </telerik:GridViewDataColumn>

                        <telerik:GridViewDataColumn
                            Header="{ResX Key=Common_ReportView_Hands_EvDiff}"
                            UniqueName="EV Diff"
                            Width="*"
                            IsResizable="True"
                            MinWidth="{Binding Path=Header, RelativeSource={RelativeSource Self}, Converter={StaticResource TextToWidthConverter}}"
                            DataMemberBinding="{Binding Path=Statistic.EVDiff}">
                            <telerik:GridViewDataColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock
                                        Text="{Binding Path=Statistic.EVDiff, StringFormat=\{0:c2\}}"
                                        Foreground="{Binding Path=Statistic.EVDiff, Converter={StaticResource ValueToColorConverter}}" />
                                </DataTemplate>
                            </telerik:GridViewDataColumn.CellTemplate>
                        </telerik:GridViewDataColumn>

                    </telerik:RadGridView.Columns>
                </telerik:RadGridView>

            </Grid>
        </Grid>

    </Grid>
</UserControl>