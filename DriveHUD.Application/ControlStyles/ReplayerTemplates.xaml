                            <TextBlock x:Name="textBlock" Text="{Binding Pot, Converter = {StaticResource PotConverter}}"  Foreground="{TemplateBinding Foreground}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
